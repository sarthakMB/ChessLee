  "description": "",
  "dependencies": {
    "@tailwindcss/cli": "^4.1.17",
    "bcrypt": "^6.0.0",
    "chess.js": "^1.4.0",
    "connect-redis": "^9.0.0",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "pg": "^8.16.3",
    "redis": "^5.10.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
